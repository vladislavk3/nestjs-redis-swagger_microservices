{
  "name": "knowin",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/knowin/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.9.4",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/microservices": "^6.11.7",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^6.11.7",
    "@nestjs/platform-ws": "^7.0.11",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^4.7.9",
    "@nestjs/websockets": "^6.11.7",
    "@sentry/node": "^5.15.5",
    "@shelf/jest-mongodb": "^1.2.1",
    "@ssnxd/socketio-jwt": "^4.5.1",
    "@types/socketio-jwt": "0.0.0",
    "aws-sdk": "^2.675.0",
    "axios": "^0.21.1",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express-rate-limit": "^5.1.3",
    "firebase-admin": "^8.10.0",
    "gtoken": "^5.0.1",
    "helmet": "^3.22.0",
    "in-app-purchase": "^1.11.4",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "mongoose": "^5.9.0",
    "nest-raven": "^6.0.0-alpha.2",
    "password-generator": "^2.3.2",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.2.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^3.4.0",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/in-app-purchase": "^1.11.0",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^13.9.1",
    "@types/redis": "^2.8.15",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.25",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "jest": "^25.1.0",
    "lint-staged": "^10.2.11",
    "mongodb-memory-server": "^6.6.1",
    "node-mocks-http": "^1.8.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-stage": "npm run format",
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "knowin/status-codes/(.*)": "<rootDir>/libs/status-codes/src/$1",
      "knowin/status-codes": "<rootDir>/libs/status-codes/src",
      "knowin/common/(.*)": "<rootDir>/libs/common/src/$1",
      "knowin/common": "<rootDir>/libs/common/src"
    },
    "preset": "@shelf/jest-mongodb"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.ts": "prettier --write"
  }
}
